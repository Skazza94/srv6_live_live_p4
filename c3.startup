ip link set eth0 address 00:00:00:c3:c1:00
ip link set eth1 address 00:00:00:c3:e2:00

ip addr add c3::e2/64 dev eth0

make all
simple_switch -i 1@eth0 -i 2@eth1 /build/srv6_forward.json &

while [[ $(pgrep simple_switch) -eq 0 ]]; do sleep 1; done
until simple_switch_CLI <<< "help"; do sleep 1; done

simple_switch_CLI <<< $(cat commands.txt)
